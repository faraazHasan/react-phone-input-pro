{
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "moduleNameMapper": {
        "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node", "css"],
    "testEnvironment": "jsdom"
  }